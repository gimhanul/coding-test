    "N = int(input())\n",
    "times = list(map(int, input().split()))\n",
    "result = 0\n",
    "\n",
    "times.sort()\n",
    "\n",
    "for i in range(N):\n",
    "    result += sum(times[:i+1])\n",
    "\n",
    "print(result)"
